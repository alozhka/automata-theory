Checks: >
  bugprone-*,
  clang-analyzer-*,
  -clang-analyzer-cplusplus*,
  -clang-analyzer-optin*,
  performance-*,
  portability-*,
  -readability-magic-numbers,
  -readability-identifier-length

WarningsAsErrors: ''
HeaderFilterRegex: '.*'
FormatStyle: file
CheckOptions:
  - key: readability-function-cognitive-complexity
    value: '50'
  - key: readability-function-size
    value: '100'